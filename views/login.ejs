<!doctype html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
 	<title><%= title %></title>
 	<link rel='stylesheet' href='/stylesheets/login.css' />
 	
 	<script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
	<script src="/socket.io/socket.io.js"></script>
	<script src="/javascripts/login.js"></script>
</head>
<body>
	<div class="global_wrapper">
		<div class="form_wrapper">
		
			<div class="login_wrapper">
				<h1>Login</h1>
				<form class="login-form">
					Email :<input type="text" name="login-email" class="login-email"><br/>
					Password :<input type="text" name="login-pw" class="login-pw"><br/>
					<input type="button" name="login-btn" class="login-btn" value="Login">
				</form>
			</div>
			
			<div class="registration_wrapper">
				<h1>Registration</h1>
				<form class="registration-form">
					Email :<input type="text" name="registration-email" class="registration-email"><br/>
					Password :<input type="text" name="registration-pw" class="registration-pw"><br/>
					Confirm Password :<input type="text" name="registration-cpw" class="registration-cpw"><br/>
					Name :<input type="text" name="registration-name" class="registration-name"><br/>
					<input type="button" name="registration-btn" class="registration-btn" value="Registration">
				</form>
			</div>
			
		</div>
	</div>
</body>
</html>